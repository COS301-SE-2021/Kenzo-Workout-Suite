  "scripts": {
    "coverage": "jest --collect-coverage",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --coverage --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:workoutIntegration": "jest --coverage --detectOpenHandles --forceExit --testPathPattern=test/Workout/IntegrationTests",
    "test:workoutTest": "jest --coverage --detectOpenHandles --forceExit --testPathPattern=test/Workout/UnitTest",
    "test:userTest": "jest --coverage --detectOpenHandles --forceExit --testPathPattern=test/User/UnitTest",
    "test:clientTest": "jest --coverage --detectOpenHandles --forceExit --testPathPattern=test/Client-Contact/UnitTest"
